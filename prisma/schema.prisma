generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Post {
  id     String @id @default(auto()) @map("_id") @db.ObjectId
  image String
  description String
  slug String @unique
  title  String
  userId String @db.ObjectId
}

model User {
  id       String @id @default(auto()) @map("_id") @db.ObjectId
  username String
  email    String @unique
  password String
}
